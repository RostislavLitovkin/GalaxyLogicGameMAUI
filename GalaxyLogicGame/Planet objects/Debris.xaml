             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:particles="clr-namespace:GalaxyLogicGame.Particles"
                            x:Class="GalaxyLogicGame.Planet_objects.Debris"
                            AbsoluteLayout.LayoutBounds="0.5, 0.5, 90, 90"
                AbsoluteLayout.LayoutFlags="PositionProportional">
    <Image Source="debris.png" x:Name="debris" AbsoluteLayout.LayoutBounds="0.5, 0.5, 60, 60"
                AbsoluteLayout.LayoutFlags="PositionProportional"/>
    <particles:DustParticle x:Name="dustParticle"/>
    <particles:DustParticle x:Name="dustParticle2"/>

    <planet_objects:BinaryIndicator x:Name="binary" IsVisible="false" BinaryString="X"/>
